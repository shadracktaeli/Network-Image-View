<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="NetworkImageView">
        <attr name="imageUrl" format="string" />
        <attr name="placeholderDrawable" format="reference" />
        <attr name="errorDrawable" format="reference" />
        <attr name="cacheStrategy" format="enum">
            <enum name="all" value="0" />
            <enum name="auto" value="1" />
            <enum name="data" value="2" />
            <enum name="none" value="3" />
            <enum name="resource" value="4" />
        </attr>
        <attr name="showProgressLoader" format="boolean" />
    </declare-styleable>
</resources>